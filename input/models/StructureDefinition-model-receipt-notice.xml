      <path value="ReceiptNotice.receiver" />
      <min value="0" />
      <max value="1" />
      <type>
        <code value="Reference" />
      </type>
    </element>
    <element id="ReceiptNotice.receiverLocation">
      <path value="ReceiptNotice.receiverLocation" />
      <min value="0" />
      <max value="1" />
