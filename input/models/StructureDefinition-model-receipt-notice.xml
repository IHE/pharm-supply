    <element id="ReceiptNotice.requestIdentifier">
      <path value="ReceiptNotice.requestIdentifier" />
      <min value="0" />
      <max value="1" />
      <type>
        <code value="Identifier" />
      </type>
    </element>

    <element id="ReceiptNotice.shipmentIdentifier">
      <path value="ReceiptNotice.shipmentIdentifier" />
